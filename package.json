{
  "name": "backend_cidades",
  "version": "1.0.0",
  "main": "./src/Server/server.ts",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon .",
    "start": "node ./dist/Server/server.js",
    "postinstall": "sucrase ./src -d ./dist --transforms typescript,imports",
    "test": "jest",
    "knex:migrate": "knex --knexfile ./src/db/knex/Environment.ts migrate:latest",
    "knex:seeds": "knex --knexfile ./src/db/knex/Environment.ts seed:run",
    "knex:rollback:all": "knex --knexfile ./src/db/knex/Environment.ts migrate:rollback --all",
    "knex:rollback": "knex --knexfile ./src/db/knex/Environment.ts migrate:rollback"
  },
  "dependencies": {
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "knex": "^3.1.0",
    "sqlite3": "^5.1.7",
    "sucrase": "^3.35.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "ts-jest": "^29.1.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/express": "^4.17.21",
    "@types/supertest": "^6.0.2",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.3",
    "sqlite": "^5.1.1",
    "supertest": "^6.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "20.x"
  }
}
